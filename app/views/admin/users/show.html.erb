<body>
  <div>
    <h1>ユーザー詳細</h1>
     <% if @user.image.attached? %>
      <%= image_tag @user.image, size: "100x100" %>
     <% else %>
      <%= image_tag 'no_image', size: "100x100" %>
     <% end %>
     <%= @user.name %>
     <p>
       <%= link_to "レビューを見る", admin_user_review_index_path %>
       <%= link_to "削除", admin_user_path, method: :delete %>
     </p>
  </div>
</body>